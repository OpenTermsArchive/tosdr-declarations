{
  "name": "Atlassian",
  "documents": {
    "Copyright Claims Policy": {
      "fetch": "https://trello.com/legal/dmca",
      "select": "div.layout-centered-content"
    },
    "Privacy Policy": {
      "fetch": "https://www.atlassian.com/legal/privacy-policy",
      "select": "body"
    },
    "Merged": {
      "fetch": "https://www.atlassian.com/legal/cloud-terms-of-service",
      "select": "html > body > main > div:nth-of-type(2) > div > div:nth-of-type(2)"
    },
    "Terms of Service": {
      "fetch": "https://www.atlassian.com/legal/cloud-terms-of-service",
      "select": "body"
    },
    "Trackers Policy": {
      "fetch": "https://www.atlassian.com/legal/cookies",
      "select": "body"
    }
  }
}