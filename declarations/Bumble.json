{
  "name": "Bumble",
  "documents": {
    "Privacy Policy": {
      "fetch": "https://bumble.com/en/privacy",
      "select": "div.formatted-text"
    },
    "Terms of Service": {
      "fetch": "https://bumble.com/en/terms",
      "select": "div.formatted-text"
    },
    "Trackers Policy": {
      "fetch": "https://bumble.com/cookie-policy",
      "select": "html > body > div:nth-of-type(2) > div > div > div:nth-of-type(2)"
    }
  }
}